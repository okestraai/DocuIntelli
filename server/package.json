    "multer": "^1.4.5-lts.1",
    "openai": "^4.24.1",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
